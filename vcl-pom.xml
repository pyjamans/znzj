		</dependency>


        <dependency>
            <groupId>com.iflytek.vie</groupId>
            <artifactId>hot-words-analysis-api</artifactId>
            <version>0.0.4-znzjBZ-SNAPSHOT</version>
        </dependency>


		<dependency>
			<groupId>com.iflytek.vie</groupId>
			<artifactId>vie-data-intergration-api</artifactId>
			<version>1.0.4-znzjBZ-RELEASE</version>
		</dependency>

		<dependency>
			<groupId>com.amazonaws</groupId>
			<artifactId>aws-java-sdk-s3</artifactId>
			<version>1.11.415</version>
		</dependency>

        <dependency>
            <groupId>org.bouncycastle</groupId>
            <artifactId>bcprov-jdk15on</artifactId>
            <version>RELEASE</version>
        </dependency>

		<!-- UAP -->
		<dependency>
			<groupId>com.iflytek.sec</groupId>
			<artifactId>uap-client</artifactId>
			<version>${uap.version}</version>
			<exclusions>
				<exclusion>
					<groupId>com.iflytek.sec</groupId>
					<artifactId>uap-cache</artifactId>
				</exclusion>
            </exclusions>
		</dependency>
		<dependency>
			<groupId>com.iflytek.sec</groupId>
			<artifactId>uap-cache2</artifactId>
			<version>${uap.version}</version>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-jersey</artifactId>
            <exclusions>
                <exclusion>
                    <artifactId>javassist</artifactId>
                    <groupId>org.javassist</groupId>
                </exclusion>
            </exclusions>
        </dependency>

		<!-- dubbo配置 -->
        <dependency>
            <groupId>org.apache.dubbo</groupId>
            <artifactId>dubbo-spring-boot-starter</artifactId>
            <version>2.7.12</version>
        </dependency>

		<!--zookeeper配置-->
		<dependency>
			<groupId>org.apache.zookeeper</groupId>
